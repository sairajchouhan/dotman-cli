{
  "name": "dotman-cli",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "dotman": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "test": "vitest run",
    "test:bun": "bun test",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "biome format --write",
    "check": "biome check",
    "check:write": "biome check --write",
    "clean": "rm -rf ./node_modules ./coverage/ ./dist/",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@1password/sdk": "^0.3.1",
    "@bitwarden/sdk-napi": "^1.0.0",
    "@clack/prompts": "^0.11.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/pacer": "^0.10.0",
    "commander": "^14.0.1",
    "dotenv": "^16.6.1",
    "dotenv-stringify": "^3.0.1",
    "ink": "^6.3.1",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "neverthrow": "^8.2.0",
    "react": "^19.2.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.18.1",
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@types/bun": "latest",
    "@types/node": "^22.18.10",
    "@types/react": "^19.2.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "globals": "^16.4.0",
    "lefthook": "^1.13.6",
    "react-devtools-core": "^7.0.1",
    "semantic-release": "^24.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "vitest": "^3.2.4"
  }
}
